{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/angular-vision/browser"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/" 
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
